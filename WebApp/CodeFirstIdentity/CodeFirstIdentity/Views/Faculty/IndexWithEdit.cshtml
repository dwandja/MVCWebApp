@model IEnumerable<CodeFirstIdentity.ViewModels.FacultyName>

@{
    ViewBag.Title = "Index";
}

<h2>All Facultys</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FullName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FullName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.FacultyId }) |
            @Html.ActionLink("Details", "Details", new { id=item.FacultyId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.FacultyId })
        </td>
    </tr>
}

</table>
